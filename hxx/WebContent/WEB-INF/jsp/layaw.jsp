	<script type="text/javascript">
	function style(){
		$("[id='style']").attr("class","1");
	}
	
	window.onload=function(){
		$("#fig").load("toResourceList.action");
	}
	
	function allResource(){
		$("#fig").empty();
		style();
		$(this).children().attr("class","active");
		$("#fig").load("toResourceList.action");
	}
	function CustomerData(){
		$("#fig").empty();
		style();
		$(this).children().attr("class","active");
		$("#fig").load("toCustomerList.action");
	}
	function UpdateUser(id){
		$("#fig").empty();
		style();
		$(this).children().attr("class","active");
		$("#fig").load("toupdateUser.action?uId="+id);
	}
	function addUser(){
		$("#fig").empty();
		style();
		$(this).children().attr("class","active");
		$("#fig").load("toaddUsers.action");
	}
	
	
	
